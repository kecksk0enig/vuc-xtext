        eType="#//AddExpr" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="KonOp">
    <eStructuralFeatures xsi:type="ecore:EReference" name="op" eType="#//RelOp" containment="true"/>
